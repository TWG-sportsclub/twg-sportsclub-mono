{
  "name": "sportsclub",
  "version": "1.0.0",
  "description": "Make every game exciting",
  "engines": {
    "node": ">=20.9.0",
    "pnpm": ">=8.15.4",
    "npm": "Please use pnpm"
  },
  "scripts": {
    "prepare": "husky",
    "build": "pnpm exec nx run-many --target=build",
    "format": "prettier \"{apps,libs}/**/*.{ts,tsx,js,jsx,mjs,md,html,css,json}\" --ignore-unknown --ignore-path .gitignore",
    "format:check": "pnpm format --check",
    "format:write": "pnpm format --write",
    "lint": "pnpm exec nx run-many --target=lint --all=true",
    "typecheck": "pnpm exec nx run-many --target=typecheck --all=true",
    "validate": "pnpm format:write && pnpm typecheck && pnpm lint"
  },
  "keywords": [],
  "author": "Antonio Reid",
  "license": "ISC",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-emoji-conventional"
    }
  },
  "dependencies": {
    "@nestjs/common": "^10.3.7",
    "@nestjs/core": "^10.3.7",
    "@nestjs/microservices": "^10.3.7",
    "@nestjs/platform-express": "^10.3.7",
    "@nx/workspace": "18.2.3",
    "buffer-from": "^1.1.2",
    "eslint-plugin-jest": "^28.2.0",
    "eslint-plugin-jsonc": "^2.15.0",
    "globals": "^15.0.0",
    "methods": "^1.1.2",
    "next": "^14.1.4",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.0",
    "snazzy": "^9.0.0",
    "source-map": "^0.7.4",
    "standard": "^17.1.0",
    "tslib": "^2.6.2"
  },
  "pnpm": {
    "overrides": {
      "express@<4.19.2": ">=4.19.2",
      "webpack-dev-middleware@>=6.0.0 <6.1.2": ">=6.1.2"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "~9.0.0",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.2",
    "@nx/cypress": "18.2.3",
    "@nx/eslint": "18.2.3",
    "@nx/eslint-plugin": "18.2.3",
    "@nx/jest": "18.2.3",
