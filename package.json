{
  "name": "sportsclub",
  "version": "1.0.0",
  "description": "Make every game exciting",
  "engines": {
    "node": ">=20.9.0",
    "pnpm": ">=8.15.4",
    "npm": "Please use pnpm"
  },
  "scripts": {
    "prepare": "husky",
    "build": "pnpm exec nx run-many --target=build",
    "format": "prettier \"{apps,libs}/**/*.{ts,tsx,js,json}\" --ignore-path .gitignore",
    "format:check": "pnpm format --check",
    "format:write": "pnpm format --write",
    "lint": "pnpm exec nx run-many --target=lint",
    "typecheck": "pnpm exec nx run-many --target=typecheck",
    "validate": "pnpm format:write && pnpm typecheck && pnpm lint"
  },
  "keywords": [],
  "author": "Antonio Reid",
  "license": "ISC",
  "dependencies": {
    "@nestjs/common": "^10.3.7",
    "@nestjs/core": "^10.3.7",
    "@nestjs/microservices": "^10.3.7",
    "@nestjs/platform-express": "^10.3.5",
    "@nx/workspace": "18.2.1",
    "buffer-from": "^1.1.2",
    "methods": "^1.1.2",
    "next": "^14.1.4",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "reflect-metadata": "^0.2.2",
    "source-map": "^0.7.4",
    "tslib": "^2.6.2"
  },
  "pnpm": {
    "overrides": {
      "express@<4.19.2": ">=4.19.2",
      "webpack-dev-middleware@>=6.0.0 <6.1.2": ">=6.1.2"
    }
  },
  "devDependencies": {
    "@nx/eslint-plugin": "18.2.1",
    "@nx/jest": "18.2.1",
    "@nx/js": "18.2.1",
    "@nx/next": "18.2.1",
    "@nx/rspack": "^18.0.6",
    "@rspack/core": "~0.5.6",
    "@rspack/dev-server": "~0.5.6",
    "@rspack/plugin-minify": "~0.5.6",
    "@rspack/plugin-react-refresh": "~0.5.6",
    "@swc-node/register": "~1.8.0",
    "@swc/core": "~1.3.85",
    "@swc/helpers": "~0.5.2",
    "@types/jest": "^29.4.0",
    "@types/node": "~18.16.9",
    "@typescript-eslint/eslint-plugin": "7.4.0",
    "@typescript-eslint/parser": "7.4.0",
    "@wanews/nx-typescript-project-references": "^0.21.0",
    "esbuild": "^0.19.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^9.0.11",
    "jest": "^29.4.1",
    "jest-environment-node": "^29.4.1",
    "lint-staged": "^15.2.2",
    "nx": "18.2.1",
    "prettier": "^2.6.2",
    "prettier-plugin-ini": "^1.1.0",
    "react-refresh": "~0.14.0",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.3.2"
  }
}
