    "@nx/webpack": "18.2.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@rspack/core": "~0.5.9",
    "@rspack/dev-server": "~0.5.9",
    "@rspack/plugin-minify": "~0.5.9",
    "@rspack/plugin-react-refresh": "~0.5.9",
    "@svgr/webpack": "^8.0.1",
    "@swc-node/register": "~1.9.0",
    "@swc/core": "~1.4.12",
